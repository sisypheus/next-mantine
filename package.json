    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@next/eslint-plugin-next": "^12.1.4",
    "@types/node": "^17.0.23",
    "@types/react": "17.0.43",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.4",
    "jest": "^27.5.1",
